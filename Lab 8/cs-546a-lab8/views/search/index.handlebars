<title>People Found</title>
<h2>People Found</h2>
<div class="container">
    <div class="row">
        <ul class="search">
            {{#each users}}
            <li class="search">
                <a href="details/{{this.id}}" name="person">{{this.firstName}} {{this.lastName}}</a>
            </li>
            {{/each}}
        </ul>
    </div>

<div class="row">
    <a class="search" href="/">Make another search</a>
</div>


</div>
